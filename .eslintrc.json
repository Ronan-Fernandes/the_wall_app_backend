{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-useless-catch": "off",
        "no-underscore-dangle": "off",
        "quotes": ["error", "double"]
    }
}
